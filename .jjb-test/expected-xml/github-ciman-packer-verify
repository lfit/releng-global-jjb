##############################################################################
echo &quot;---&gt; packer-clear-credentials.sh&quot;

set +e  # DO NOT cause build failure if any of the rm calls fail.
rm &quot;$CLOUDENV&quot;
exit 0
</command>
    </hudson.tasks.Shell>
